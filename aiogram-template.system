[Unit]
Description=descp
After=syslog.target network.target postgresql.service
Requires=postgresql.service

[Service]
Type=simple
WorkingDirectory=/home/user/dir
ExecStart=poetry run python3 -m bot
User=User
Group=user
KillMode=process
Restart=on-failure
RestartSec=10

[Install]
WantedBy=multi-user.target